<html>
<head>
    @model List<Usuario>
    @{
        ViewData["Title"] = "User List";
    }
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css">
</head>
<body>
    <div class="d-flex align-items-center justify-content-center" style="min-height: 100vh;">
        
            <div class="card" style="width: 18rem;">
                <img src="https://www.secret-source.eu/wp-content/uploads/2017/11/C-sharp-logo.jpg" class="card-img-top" alt="Imagem da Internet">
                <div class="card-body">
                    <p class="card-text">GetApi, Utilizando C#</p>
                    <a class="btn btn-outline-success" href="@Url.Action("Pagina2","Home",new { amountProfile = false})">View</a>
                </div>
            </div>
            <div class="card" style="width: 18rem;">
                <img src="https://upload.wikimedia.org/wikipedia/commons/6/6a/JavaScript-logo.png" class="card-img-top" alt="Imagem da Internet">
                <div class="card-body">
                    <p class="card-text">GetApi, Utilizando JavaScript</p>
                    <a class="btn btn-outline-success" href="@Url.Action("Pagina1","Home")">View</a>
                </div>
            </div>
